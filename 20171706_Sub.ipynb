   "source": [
    "%%writefile Sub.py\n",
    "import re\n",
    "\n",
    "def square(match):\n",
    "    value = int(match.group())\n",
    "    return str(value*value)\n",
    "\n",
    "p = re.compile(r\"\\d+\")\n",
    "print(p.sub(square, 'Please, square the following numbers, 3 7 11 13 17 19'))"
   ]
