    "%matplotlib nbagg\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = np.linspace(-np.pi, np.pi, 256)\n",
    "c = np.cos(x)\n",
    "plt.plot(x,c)\n",
    "plt.xticks([-np.pi,-np.pi/2, 0, np.pi/2, np.pi],['$-\\pi$','$-\\pi/2$','0','$pi/2$','$\\pi$'])\n",
    "plt.yticks([-1, 0, +1])\n",
    "plt.show()"
   ]
